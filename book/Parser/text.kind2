use Parser/skip
use Parser/bind
use Parser/test
use Parser/take
use Parser/pure
use Parser/fail
use Bool/{true,false}
use String/{length}

text (txt: String) : (Parser Unit) =
  (skip _
    (bind _ _ (test txt) λsuccess
      match success {
        true: 
          (bind _ _ (take (length txt)) λx
            (pure _ Unit/new))
        false: (fail _ "Text mismatch")
      }
    )
  )
