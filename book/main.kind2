use Bits/{O,I,E}
use Shape/{circle,line,xshape}

main: (App (Board))

use init =
  let board = (Board/new [] 0 Bool/false (V2/new 0 0) (V2/new 0 0))
  board

use tick = λstate
  state

use draw = λstate

  let board = match state {
    Board/new: 
      state
  }: Board 

  (Board/draw board)


(App/new _ init tick draw Board/when)










