// Computes the absolute difference between two U48 numbers
// - a: The first U48 number
// - b: The second U48 number
// = The absolute difference between `a` and `b`

use Bool/{true,false}

abs_diff
- a: U48
- b: U48
: U48

let x = (U48/to_bool (< a b))
match x {
  true: (- b a)
  false: (- a b)
}
